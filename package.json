  "description": "FHIR Questionnaire To Json Schema Converter",
  "version": "0.0.1",
  "dependencies": {},
  "main": "./lib",
  "scripts": {
    "build": "babel src --out-dir lib",
    "lint": "eslint -f unix .",
    "inspect": "jsinspect",
    "prettier": "prettier --list-different \"./**/**.js\"",
