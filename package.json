    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-latest": "^6.24.1",
    "es6-promise": "^4.2.4",
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "isomorphic-fetch": "^2.2.1",
    "jsinspect": "^0.12.7",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "prettier": "^1.12.0"
